# licq uin to alias:
#     Convert one/many uin[s] to their corresponding alias[es]
# Usage:
#   lu <uin> [uin uin ...]
if [[ "${#@}" == "0" ]]; then
  echo "$0 (licq uin to alias): Convert uin[s] to their corresponding alias[es]
Usage:
  $0 <uin> [uin uin ...]"
else
  local uin aliases
  for uin in $@
  do
    if [[ -f ~/.licq/conf/$uin.uin ]]; then
       aliases=($aliases $(grep Alias ~/.licq/conf/$uin.uin|cut -f2 -d=))
    fi
  done
  print $aliases
fi
